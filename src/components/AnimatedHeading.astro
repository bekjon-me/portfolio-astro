<script>
    document.addEventListener('astro:page-load', () => {
        const svgContainer = document.getElementById("svg-container");
        const svgElement = document.getElementById("svg-element");
        const tmax_optionsGlobal = {
            repeat: -1,
            repeatDelay: 0.65,
            yoyo: true,
        };
        const stagger_opts_to = {
            x: 0,
            y: 0,
            opacity: 1,
            scale: 1,
            rotation: 0,
            ease: "power4.easeInOut",
        };
        const timeline = gsap.timeline(tmax_optionsGlobal);
        const path = "svg *";
        const stagger_val = 0.0125;
        const duration = 1.5;

        const getRandomPosition = (min: number, max: number) => {
            return Math.random() * (max - min) + min;
        };

        function animate() {
            if (!svgContainer) return;
            svgContainer?.classList.remove("hidden");
            Array.from(svgElement.querySelectorAll(path)).forEach((el) => {
                timeline.set(el, {
                    x: `+=${getRandomPosition(-500, 500)}`,
                    y: `+=${getRandomPosition(-500, 500)}`,
                    rotation: `+=${getRandomPosition(-720, 720)}`,
                    scale: 0,
                    opacity: 0,
                });
            });

            timeline.to(path, duration, stagger_opts_to, stagger_val);
        }

        function onAnimateHover() {
            svgContainer?.addEventListener("mouseenter", () => {
                timeline.timeScale(0.25);
            });

            svgContainer?.addEventListener("mouseleave", () => {
                timeline.timeScale(1);
            });
        }

        onAnimateHover();
        animate();
    });
</script>
